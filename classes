# classes.py

class usuario:
    def __init__(self, nome, endereco, telefone, cpf_cnpj, email):
        self.nome = nome
        self.endereco = endereco
        self.telefone = telefone
        self.cpf_cnpj = cpf_cnpj
        self.email = email

    def __str__(self):
        return (f"--- usuario ---\n"
                f"Nome: {self.nome}\n"
                f"CPF/CNPJ: {self.cpf_cnpj}\n"
                f"Email: {self.email}\n"
                f"Telefone: {self.telefone}\n"
                f"Endere√ßo: {self.endereco}\n")

